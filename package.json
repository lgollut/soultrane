{
  "name": "soultrane",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "module": "./dist/soultrane.js",
  "exports": {
    ".": {
      "import": "./dist/soultrane.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier '**/!(CHANGELOG).@(ts|tsx|md|mdx)' --list-different",
    "prettier:fix": "npm run prettier -- --write",
    "tsc": "tsc --noEmit",
    "validate": "npm-run-all eslint prettier --continue-on-error",
    "format": "npm-run-all eslint:fix prettier:fix --continue-on-error",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest run",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.0.0-alpha.7",
    "@storybook/addon-interactions": "^8.0.0-alpha.7",
    "@storybook/addon-links": "^8.0.0-alpha.7",
    "@storybook/blocks": "^8.0.0-alpha.7",
    "@storybook/react": "^8.0.0-alpha.7",
    "@storybook/react-vite": "^8.0.0-alpha.7",
    "@storybook/test": "^8.0.0-alpha.7",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.3",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-testing-library": "^6.2.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^23.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "storybook": "^8.0.0-alpha.7",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.1.3"
  }
}
